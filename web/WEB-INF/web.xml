<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>recibir_SA</servlet-name>
        <servlet-class>controladores.recibir_SA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mostrar</servlet-name>
        <servlet-class>controladores.Mostrar_CA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modificar_SA</servlet-name>
        <servlet-class>controladores.modificar_SA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mostrar_CA</servlet-name>
        <servlet-class>controladores.Mostrar_CA</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>recibir_SA</servlet-name>
        <url-pattern>/recibir_SA.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mostrar</servlet-name>
        <url-pattern>/mostrar.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modificar_SA</servlet-name>
        <url-pattern>/modificar_SA.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mostrar_CA</servlet-name>
        <url-pattern>/Mostrar_CA.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
